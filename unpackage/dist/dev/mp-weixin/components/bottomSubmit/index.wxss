.bottom-submit-style button {
  border: none;
}
.bottom-submit-style .btn-class {
  height: 41px;
  line-height: 40px;
  font-size: 14px;
}
.bottom-submit-style .submit-class {
  border-top: 1px solid #eee;
  height: 40px;
  line-height: 40px;
}
.bottom-submit-style .custom-popup {
  margin-bottom: 39px !important;
  background: #FFFFFF;
}
.bottom-submit-style .bar-class {
  height: 40px;
}
.bottom-submit-style .customH {
  height: 40vh;
  overflow-y: scroll;
}
.bottom-submit-style .iconCard {
  position: relative;
  width: 38px;
  height: 38px;
  /* background: #ffa006; */
  border-radius: 50%;
  text-align: center;
  line-height: 43px;
  color: #fff;
  font-size: 24px;
  margin: 0 0 20px 10px;
}
.bottom-submit-style .tapCard {
  font-size: 12px;
  color: #707070;
  margin-left: 10px;
}
.bottom-submit-style .popup-top {
  font-size: 10pt;
  background: #eee;
  height: 30px;
  line-height: 30px;
  padding: 0 10px;
  color: #0f0f0f;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.bottom-submit-style .popup-topRight {
  vertical-align: middle;
  color: #707070;
}
.bottom-submit-style .cartListStyle {
  max-height: 50vh;
  overflow-y: scroll;
}
.bottom-submit-style .custom-class-row {
  font-size: 12pt;
  border-bottom: 1px solid #eee;
  padding: 10px 10px 16px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.bottom-submit-style .custom-class-row .oneColFa {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.bottom-submit-style .oneCol {
  font-size: 11pt;
  line-height: 30px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.bottom-submit-style .partsColH {
  line-height: 32px;
}
.bottom-submit-style .partsCol {
  display: inline-block;
}
.bottom-submit-style .partsColview {
  width: 100%;
  display: inline-block;
  color: #707070;
  font-size: 12px;
  word-break: normal;
  /* overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; */
}
.bottom-submit-style .partsCol {
  display: inline-block;
  color: #707070;
  font-size: 12px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.bottom-submit-style .twoCol {
  text-align: center;
  /* color:#ffa006;  */
  line-height: 44px;
  margin: auto 15px;
}
.bottom-submit-style .threeCol {
  margin: 20px 0;
}
.bottom-submit-style .stepperStyle {
  display: inline-block;
  margin-left: 5px;
  margin: auto;
}
.bottom-submit-style .iconCardRight {
  position: absolute;
  top: 0;
  right: 5px;
  min-width: 16px;
  padding: 0 3px;
  font-family: PingFang SC, Helvetica Neue, Arial, sans-serif;
  font-size: 12px;
  font-weight: 500;
  line-height: 14px;
  color: #fff;
  text-align: center;
  white-space: nowrap;
  border: 1px solid #fff;
  border-radius: 16px;
  -webkit-transform: translateX(50%);
  transform: translateX(50%);
  box-sizing: border-box;
  -webkit-transform-origin: 100%;
  transform-origin: 100%;
}

