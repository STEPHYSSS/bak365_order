.index-canteen-style {
  background: #FFFFFF;
  min-height: 100vh;
}
.index-canteen-style .customer-service-style {
  right: 35px;
  z-index: 5;
}
.index-canteen-style .uni-popup {
  z-index: 10;
}
.index-canteen-style .homepage {
  padding-top: 79px;
  background: #FFFFFF;
}
.index-canteen-style .shop-name {
  text-align: center;
  font-size: 14px;
  padding-top: 5px;
  line-height: 25px;
  position: fixed;
  width: 100%;
  z-index: 1;
  background: #fff;
}
.index-canteen-style .shop-name-icon {
  display: inline-block;
  vertical-align: middle;
  margin-right: 4px;
}
.index-canteen-style .homepage .homePageTop {
  /* position:fixed;  */
  /* top:0; */
  width: 100%;
  z-index: 3;
}
.index-canteen-style .homepageLeftFa {
  height: 100vh;
  position: fixed;
  left: 0;
  min-width: 74px;
  max-width: 74px;
  overflow-y: scroll;
  background: #f9f9f9;
}
.index-canteen-style .homepageLeftFixed {
  font-size: 11pt;
  margin-bottom: 96px;
  color: #707070;
  font-weight: 500;
  background: #f9f9f9;
  padding-bottom: 15px;
}
.index-canteen-style .homepageLeft {
  background: #f7f6f6;
  text-align: center;
  padding: 12px 8px;
  line-height: 1.4;
  position: relative;
}
.index-canteen-style .homepageLeft-info {
  position: absolute;
  top: 5px;
  right: 10px;
  width: 10px;
  height: 10px;
  /* padding: 0 3px; */
  /* font-family: PingFang SC, Helvetica Neue, Arial, sans-serif; */
  font-size: 12px;
  font-weight: 500;
  line-height: 14px;
  color: #fff;
  text-align: center;
  white-space: nowrap;
  border: 1px solid #fff;
  border-radius: 16px;
  -webkit-transform: translateX(50%);
  transform: translateX(50%);
  box-sizing: border-box;
  -webkit-transform-origin: 100%;
  transform-origin: 100%;
}
.index-canteen-style .homepageRight {
  margin-left: 74px;
}
.index-canteen-style .homepage .iconCard {
  width: 44px;
  height: 44px;
  background: #ffa006;
  border-radius: 50%;
  text-align: center;
  line-height: 50px;
  color: #fff;
  font-size: 25px;
  margin: 0 0 20px 20px;
}
.index-canteen-style .homepage .tapCard {
  font-size: 12px;
  color: #707070;
  margin-left: 10px;
}
.index-canteen-style .custom-popup {
  margin-bottom: 50px;
}
.index-canteen-style .MockRadio {
  padding: 10px;
  display: block;
}
.index-canteen-style .MockLabel {
  display: block;
  width: 100%;
  line-height: 20px;
}
.index-canteen-style .indexTop {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin: 10px 0;
}
.index-canteen-style .changeMode {
  width: 130px;
  height: 60px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background: #eee;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  border-radius: 10px;
  font-size: 14px;
  color: #3062a5;
}
.index-canteen-style .changeModeImg {
  width: 35px !important;
  height: 35px;
}
.index-canteen-style .changeModeLeft {
  margin-right: 20px;
}
.index-canteen-style .changeModeRight {
  margin-left: 10px;
}
.index-canteen-style .isActive {
  border: 1px solid #3062a5;
}
.index-canteen-style .shop-revert {
  float: left;
  position: absolute;
  left: 5px;
  color: #777;
}
.index-canteen-style .shop-revert-icon {
  vertical-align: middle;
}
.index-canteen-style .ReachTime {
  padding: 5px;
  border-bottom: 1px solid #eee;
  border-top: 1px solid #eee;
  background: #fff;
  position: relative;
  height: 35px;
}
.index-canteen-style .ReachTime .ReachTimeInput {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.index-canteen-style .ReachTime .weui-input {
  width: 100%;
  background: #eeeeee;
  border-radius: 15px;
  text-align: left;
  font-size: 11pt;
  height: 30px;
  padding: 2px 12px;
}
.index-canteen-style .goodCodeStyle {
  position: absolute;
  right: 8px;
  top: 4px;
  font-size: 22px;
  width: 39px;
  height: 33px;
  line-height: 33px;
  z-index: 5;
}

