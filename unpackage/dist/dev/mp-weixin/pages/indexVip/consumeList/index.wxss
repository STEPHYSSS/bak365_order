.consume-style .text-center {
  text-align: center;
  font-size: 10pt;
}
.consume-style .text-black {
  color: #333333;
}
.consume-style .bg-white {
  background-color: #ffffff;
  border-bottom: 1px solid #eeeeee !important;
}
.consume-style .listTemplate {
  border-bottom: 6px solid #eee;
  padding: 8px 15px;
  color: #5f5f5f;
  font-size: 10pt;
  line-height: 2;
  background: #FFFFFF;
}
.consume-style .operation-time {
  border-bottom: 1px solid #eee;
  padding-bottom: 8px;
}
.consume-style .opera-time-title {
  color: #ee0a24;
}
.consume-style .operation-info {
  position: relative;
  padding: 4px 0 0 10px;
}
.consume-style .h3 {
  line-height: 1.5;
}
.consume-style .shop-consume {
  position: absolute;
  top: 10px;
  right: 10px;
  color: #f08300;
}
.consume-style .order-number {
  color: #f08300;
}

