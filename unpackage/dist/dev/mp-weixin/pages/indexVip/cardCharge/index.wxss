.card-charge-style .uni-popup__wrapper {
  background: #fff;
}
.card-charge-style .charge-store {
  font-size: 10pt;
  color: #777;
  line-height: 25px;
  margin-left: 10px;
  text-align: center;
  margin-top: 10px;
}
.card-charge-style .titleDialog {
  display: inline-block;
  width: 100%;
  height: 34px;
  line-height: 34px;
  text-align: center;
  font-size: 13pt;
}
.card-charge-style .contentDialog {
  word-wrap: break-word;
  word-break: normal;
  background: #eeeeee;
  padding: 10px 20px;
  border-radius: 20px;
  font-size: 12pt;
}
.card-charge-style .contentDialog ._ol {
  list-style-type: none;
  counter-reset: sectioncounter;
}
.card-charge-style .contentDialog ._ol ._li:first-child {
  margin-top: 15px;
}
.card-charge-style .contentDialog ._ol ._li:before {
  content: counter(sectioncounter) "、";
  counter-increment: sectioncounter;
}
.card-charge-style .contentDialog ._span:first-child {
  display: inline-block;
  font-weight: bold;
}
.card-charge-style .weiFullCenter {
  padding: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.card-charge-style .weiFullCenter .IncomeList-style {
  width: 50%;
}
.card-charge-style .weiFullCenter .backColor {
  background: rgba(253, 204, 99, 0.3);
}
.card-charge-style .weiFullCenter .backBorder {
  border: 1px solid #fdcc63 !important;
}
.card-charge-style .weiFullCenter .weiFullBox {
  border-radius: 10px;
  box-shadow: 1px 1px 3px 3px #e0e0e0;
  border: 1px solid #fff;
  height: 80px;
  margin-bottom: 10px;
  padding: 0 20px;
}
.card-charge-style .weiFullCenter .weiFullBox .moneyTop {
  display: inline-block;
  font-size: 20px;
  font-weight: bold;
  margin-top: 20px;
  line-height: 18px;
}
.card-charge-style .weiFullCenter .weiFullBox .moneyBottom {
  display: inline-block;
  margin-top: 4px;
  width: 90%;
  /*加省略号*/
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 14px;
  color: #666666;
}
.card-charge-style .weiFullCenter .weiFullBox .amountBoxImg {
  width: 100%;
  height: 100%;
  border-radius: 10px;
}
.card-charge-style .weiFullCenter .IncomeList-style:nth-child(2n) .weiFullBox {
  margin-left: 5px !important;
}
.card-charge-style .weiFullCenter .IncomeList-style:nth-child(2n-1) .weiFullBox {
  margin-right: 5px !important;
}

